

	<!-- footer -->
	<%- include('../partials/header')%>
	<div class="site-section">
				
		<div class="container">
			<% if (locals.countInWishlist>0) { %>
		  <div class="row mb-5">
			<form class="col-md-12" method="post">
			  <div class="site-blocks-table">
				<table class="table ">
				  <thead>
					<tr>
					  <th scope="col" class="product-thumbnail">Image</th>
					  <th scope="col" class="product-name">Product</th>
					  <th scope="col" class="product-price">Price</th>
					  <th scope="col" class="product-addCart">Add to Cart</th>
					  <th scope="col" class="product-remove">Remove</th>
					</tr>
				  </thead>
				  <tbody>
					<% wishlistData.forEach((product)=>{%>
					<tr>
					  <td class="product-thumbnail">
						<a href="/shop"><img src="/adminImages/<%= product.productDetail._id %>.jpg" alt="Image" class="img-fluid"></a>
					  </td>
					  <td class="product-name">
						<h2 class="h5 text-black"><%= product.productDetail.name %></h2>
					  </td>
					  <td><%= product.productDetail.price%> â‚¹</td>
					  <td class="addToCart"><a style="color:#fff" class="btn btn-warning" href="/cart/<%= product.productDetail._id %> ">Add to Cart</a></td>
					  <td class="product-remove">
					 <button class="btn btn-danger" style="color:#fff"
					 onclick="removeFromWishlist('<%= product._id %>','<%= product.productDetail._id %>')">
					 remove</button>
					</td>
					</tr>
	
					<% }) %>
				  
				  </tbody>
				</table>
			  </div>
			</form>
		  </div>
		  <% } else { %>
			<h2 class="text-center text-danger">Oops!<br>
				Your Cart is empty </h3>
			  <div class=""><img src="/adminImages/emptyCart.png" class="img-fluid rounded mx-auto d-block mb-5" alt="image">
			   
			 
			  </div>
		  <% } %>
	   </div>
		<!-- end wishlist -->
	
		<!-- footer -->
		<%- include('../partials/footer')%>
			